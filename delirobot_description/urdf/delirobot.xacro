            </geometry>
        </collision>
    </link>
    <joint name="lidar_joint" type="fixed">
        <origin xyz="0 0 0.36" rpy="0 0 0" />
        <parent link="body" />
        <child link="hokuyo" />
        <axis xyz="0 0 0" />
    </joint>

    <gazebo reference="hokuyo">
        <material>Gazebo/Gold</material>
    </gazebo>
