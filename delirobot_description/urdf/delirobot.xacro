    <!-- ============================================ bulb ============================================ -->
    <link name="bulb">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0.006" />
            <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://delirobot_description/meshes/bulb.STL" />
            </geometry>
            <material name="">
                <color rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://delirobot_description/meshes/bulb.STL" />
            </geometry>
        </collision>

        <light name='lamp' type='spot'>
        </light>
        <plugin name='light_control' filename='libLedPlugin.so'>
            <light>
                <light>/lamp</light>
                <duration>0.5</duration>
                <interval>0.5</interval>
            </light>
        </plugin>

    </link>
    <joint name="bulb_joint" type="fixed">
        <origin xyz="-0.19137 0 0.25319" rpy="0 0 0" />
        <parent link="base_link" />
        <child link="bulb" />
        <axis xyz="0 0 0" />
    </joint>
    <!-- ============================================ plate ============================================ -->
    <link name="plate">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0.51" />
            <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
        </inertial>
        <visual>
